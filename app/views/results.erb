<h1>Search Results</h1>
<div>
  <table>
    <tr>
      <th>Category</th>
      <th>Question</th>
      <th>Date</th>
    </tr>
    <% @all_questions.each do |question| %>
    <tr>
      <td><%= question.category.name %></td>
      <td><a href="/questions/<%= question.id %>"><%= question.title %></a></td>
      <td><%= question.created_at %></td>
    </tr>
    <% end %>
  </table>
</div>
